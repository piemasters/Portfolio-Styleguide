    "img": "assets/images/projects/crypto/cover.jpg",
    "url": "https://crypto.davidnorton.app/",
    "overview": "A crypto-currency price dashboard",
    "technologies": [
      "angular", "bootstrap", "cli", "firebase", "jasmine", "karma",
      "ng-bootstrap", "protractor", "rxjs", "sass", "typescript"
    ],
    "details": [
      {
        "id": "1",
